<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:mcpapp="http://schemas.android.com/apk/com.l4digital.fastscroll" xmlns:aapt="http://schemas.android.com/aapt" android:id="@+id/bnav_cruise_menu" android:background="@android:color/transparent" android:layout_width="match_parent" android:layout_height="match_parent">
    <com.baidu.navisdk.ui.widget.EmptyTopLayout android:id="@+id/top_empty_view" android:background="@android:color/white" android:layout_width="match_parent" android:layout_height="wrap_content"/>
    <RelativeLayout android:gravity="center_vertical" android:id="@+id/top_bar" android:background="@android:color/white" android:paddingLeft="@dimen/navi_dimens_13dp" android:paddingTop="@dimen/navi_dimens_6dp" android:paddingRight="@dimen/navi_dimens_8dp" android:paddingBottom="@dimen/navi_dimens_6dp" android:visibility="visible" android:layout_width="match_parent" android:layout_height="@dimen/navi_dimens_54dp" android:layout_below="@+id/top_empty_view">
        <ImageView android:id="@+id/back_arrow" android:background="@drawable/nsdk_ugc_icon_back" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_centerVertical="true"/>
        <TextView android:textSize="@dimen/navi_dimens_15dp" android:textStyle="normal" android:textColor="#000000" android:id="@+id/title" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/nsdk_future_trip_title" android:layout_centerInParent="true"/>
    </RelativeLayout>
    <include android:id="@+id/panel_container" android:layout_width="match_parent" android:layout_height="@dimen/nsdk_future_trip_main_panel_height" android:layout_marginLeft="@dimen/navi_dimens_5dp" android:layout_marginRight="@dimen/navi_dimens_5dp" android:layout_marginBottom="@dimen/navi_dimens_5dp" android:layout_alignParentBottom="true" layout="@layout/nsdk_layout_future_trip_main_panel"/>
    <RelativeLayout android:id="@+id/map_widget_container" android:layout_width="match_parent" android:layout_height="match_parent" android:layout_above="@+id/panel_container" android:layout_below="@+id/top_bar"/>
    <RelativeLayout android:id="@+id/message_btn_container" android:background="@drawable/nsdk_drawable_rr_nomal_button_mask" android:layout_width="@dimen/navi_dimens_42dp" android:layout_height="@dimen/navi_dimens_42dp" android:layout_marginLeft="@dimen/navi_dimens_5dp" android:layout_marginTop="@dimen/navi_dimens_8dp" android:layout_below="@+id/top_bar" android:layout_alignParentLeft="true">
        <RelativeLayout android:id="@+id/message_collection" android:layout_width="@dimen/navi_dimens_24dp" android:layout_height="@dimen/navi_dimens_20dp" android:layout_marginTop="@dimen/navi_dimens_3dp" android:layout_centerHorizontal="true">
            <ImageView android:layout_width="@dimen/navi_dimens_24dp" android:layout_height="@dimen/navi_dimens_20dp" android:src="@drawable/nsdk_drawable_route_result_yellow_message" android:layout_centerInParent="true"/>
            <TextView android:textSize="@dimen/navi_dimens_5dp" android:textColor="#ffffff" android:gravity="center" android:id="@+id/message_label" android:background="@drawable/nsdk_drawable_route_result_toolbox_red_circle" android:visibility="gone" android:layout_width="@dimen/navi_dimens_8dp" android:layout_height="@dimen/navi_dimens_8dp" android:text="3" android:layout_alignParentTop="true" android:layout_alignParentRight="true"/>
        </RelativeLayout>
        <TextView android:textSize="@dimen/navi_dimens_9dp" android:textColor="@color/nsdk_route_result_button_text_color" android:ellipsize="middle" android:gravity="center" android:id="@+id/toolbox_tv" android:visibility="visible" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="消息" android:singleLine="true" android:layout_below="@+id/message_collection" android:layout_centerHorizontal="true"/>
    </RelativeLayout>
    <com.baidu.navisdk.module.future.widgets.FutureTripYellowBannerContainer android:id="@+id/single_yellow_banner_container" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="@dimen/navi_dimens_10dp" android:layout_marginRight="@dimen/navi_dimens_10dp" android:layout_below="@+id/top_bar"/>
    <RelativeLayout android:gravity="center" android:id="@+id/location_layout" android:background="@drawable/nsdk_drawable_rr_nomal_button_mask" android:clickable="true" android:layout_width="@dimen/navi_dimens_42dp" android:layout_height="@dimen/navi_dimens_42dp" android:layout_marginLeft="@dimen/navi_dimens_5dp" android:layout_marginBottom="@dimen/navi_dimens_5dp" android:layout_above="@+id/panel_container" android:layout_alignParentBottom="false">
        <ImageView android:layout_gravity="center" android:id="@+id/location_btn" android:layout_width="match_parent" android:layout_height="match_parent" android:src="@drawable/nsdk_rr_location_icon" android:scaleType="center" android:contentDescription="我的位置"/>
    </RelativeLayout>
    <RelativeLayout android:gravity="center_horizontal" android:id="@+id/level_container" android:visibility="visible" android:layout_width="wrap_content" android:layout_height="@dimen/navi_dimens_22dp" android:layout_marginLeft="@dimen/navi_dimens_6dp" android:layout_marginBottom="@dimen/navi_dimens_5dp" android:layout_toRightOf="@+id/location_layout" android:layout_alignBottom="@+id/location_layout">
        <TextView android:textSize="@dimen/navi_dimens_11dp" android:textColor="#ffffff" android:gravity="center" android:id="@+id/level_bg" android:layout_width="wrap_content" android:layout_height="match_parent" android:singleLine="true"/>
        <TextView android:textSize="@dimen/navi_dimens_11dp" android:textColor="#000000" android:gravity="center" android:id="@+id/level_tv" android:layout_width="wrap_content" android:layout_height="match_parent" android:singleLine="true"/>
        <TextView android:gravity="center" android:id="@+id/level_drawable" android:background="@drawable/nsdk_rr_level_scale_icon" android:layout_width="wrap_content" android:layout_height="@dimen/navi_dimens_6dp" android:layout_alignParentBottom="true"/>
    </RelativeLayout>
    <TextView android:textSize="13sp" android:textColor="#ff3377ff" android:gravity="center_vertical" android:id="@+id/location_setting" android:background="@drawable/nsdk_drawable_rr_nomal_button_mask" android:paddingLeft="20dp" android:paddingRight="20dp" android:layout_width="wrap_content" android:layout_height="@dimen/navi_dimens_42dp" android:layout_marginLeft="4dp" android:text="确切位置:已关闭" android:drawableRight="@drawable/nsdk_location_set_drawable" android:drawablePadding="15dp" android:layout_toRightOf="@+id/location_layout" android:layout_alignBottom="@+id/location_layout"/>
    <RelativeLayout android:id="@+id/time_picker_container" android:layout_width="match_parent" android:layout_height="wrap_content">
        <FrameLayout android:id="@+id/time_picker_inner_container" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_alignParentBottom="true"/>
    </RelativeLayout>
    <ViewStub android:id="@+id/eta_view_stub" android:layout="@layout/nsdk_layout_future_trip_eta" android:layout_width="match_parent" android:layout_height="match_parent"/>
    <ViewStub android:id="@+id/multi_yellow_view_stub" android:layout="@layout/nsdk_layout_future_trip_multi_yellow_banner" android:layout_width="match_parent" android:layout_height="match_parent"/>
    <ViewStub android:id="@+id/ugc_event_view_stub" android:layout="@layout/nsdk_layout_future_trip_ugc_banner" android:layout_width="match_parent" android:layout_height="match_parent"/>
    <FrameLayout android:id="@+id/bubble_container" android:visibility="gone" android:layout_width="match_parent" android:layout_height="match_parent"/>
</RelativeLayout>

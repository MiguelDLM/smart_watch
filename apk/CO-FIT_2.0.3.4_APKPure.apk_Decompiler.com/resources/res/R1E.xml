<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:mcpapp="http://schemas.android.com/apk/com.l4digital.fastscroll" xmlns:aapt="http://schemas.android.com/aapt" android:layout_width="match_parent" android:layout_height="match_parent">
    <androidx.cardview.widget.CardView android:id="@+id/dialog_img_container" android:layout_width="275dp" android:layout_height="360dp" android:layout_centerHorizontal="true" app:cardCornerRadius="10dp">
        <ImageView android:id="@+id/dialog_img" android:layout_width="match_parent" android:layout_height="match_parent" android:scaleType="fitXY" android:adjustViewBounds="true"/>
    </androidx.cardview.widget.CardView>
    <ImageView android:id="@+id/dialog_close" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="40dp" android:src="@drawable/nsdk_drawable_single_img_dialog_close" android:scaleType="center" android:layout_below="@+id/dialog_img_container" android:layout_centerHorizontal="true"/>
</RelativeLayout>

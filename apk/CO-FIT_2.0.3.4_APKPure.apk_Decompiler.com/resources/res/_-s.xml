<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:mcpapp="http://schemas.android.com/apk/com.l4digital.fastscroll" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_pressed="false">
        <shape>
            <solid android:color="@color/nsdk_cl_bg_d"/>
            <corners android:radius="@dimen/navi_dimens_5dp"/>
            <stroke android:width="0.5dp" android:color="@color/bnav_common_button_stroke"/>

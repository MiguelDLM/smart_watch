<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:mcpapp="http://schemas.android.com/apk/com.l4digital.fastscroll" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_checked="true">
        <shape>
            <solid android:color="#ff8c33"/>
            <corners android:radius="12dp"/>
            <stroke android:width="1dp" android:color="#ff8c33"/>
        </shape>
    </item>
    <item android:state_checked="false">
        <shape>
            <corners android:radius="12dp"/>
            <stroke android:width="1dp" android:color="#ff8c33"/>
        </shape>
    </item>
</selector>

<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:mcpapp="http://schemas.android.com/apk/com.l4digital.fastscroll" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_pressed="true">
        <shape>
            <stroke android:width="1px" android:color="#4d1fcca1"/>
            <solid android:color="#331fcca1"/>
            <corners android:radius="100dp"/>
        </shape>
    </item>
    <item android:state_pressed="false">
        <shape>
            <stroke android:width="1px" android:color="#4d1fcca1"/>
            <solid android:color="#141fcca1"/>
            <corners android:radius="100dp"/>
        </shape>
    </item>
</selector>

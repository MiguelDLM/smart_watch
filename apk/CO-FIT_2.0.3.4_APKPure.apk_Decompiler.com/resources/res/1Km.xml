<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:mcpapp="http://schemas.android.com/apk/com.l4digital.fastscroll" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <solid android:color="@android:color/transparent"/>
            <stroke android:width="0.5dp" android:color="@color/gray_A3A3A3"/>
            <corners android:radius="6dp"/>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="#2faaaaaa"/>
            <stroke android:width="0.5dp" android:color="@color/gray_A3A3A3"/>
            <corners android:radius="6dp"/>
        </shape>
    </item>
</selector>

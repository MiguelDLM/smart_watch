<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:mcpapp="http://schemas.android.com/apk/com.l4digital.fastscroll" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <solid android:color="#ffffff"/>
            <corners android:radius="10dp"/>
            <stroke android:width="1dp" android:color="#3385ff"/>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="#f2f2f2"/>
            <corners android:radius="10dp"/>
            <stroke android:width="1dp" android:color="#3385ff"/>
        </shape>
    </item>
</selector>

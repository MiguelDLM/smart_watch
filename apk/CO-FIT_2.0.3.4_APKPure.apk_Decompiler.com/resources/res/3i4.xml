<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:mcpapp="http://schemas.android.com/apk/com.l4digital.fastscroll" xmlns:aapt="http://schemas.android.com/aapt">
    <FrameLayout android:id="@+id/route_tab_view" android:layout_width="match_parent" android:layout_height="wrap_content">
        <LinearLayout android:orientation="horizontal" android:id="@+id/multi_tabs_container" android:layout_width="match_parent" android:layout_height="76dp" android:layout_marginLeft="5dp" android:layout_marginTop="7dp" android:layout_marginRight="5dp" android:layout_marginBottom="7dp">
            <com.baidu.navisdk.routetab.view.item.multi.MotorMultiRouteTabItem android:id="@+id/multi_tabs_one" android:visibility="gone" android:layout_marginLeft="2dp" android:layout_marginRight="2dp" style="@style/MultiRouteTabItem"/>
            <View android:id="@+id/multi_tabs_line1" android:background="#e9eaf0" android:layout_width="0.5dp" android:layout_height="match_parent" android:layout_marginTop="5dp" android:layout_marginBottom="5dp"/>
            <com.baidu.navisdk.routetab.view.item.multi.MotorMultiRouteTabItem android:id="@+id/multi_tabs_two" android:visibility="gone" android:layout_marginLeft="2dp" android:layout_marginRight="2dp" style="@style/MultiRouteTabItem"/>
            <View android:id="@+id/multi_tabs_line2" android:background="#e9eaf0" android:layout_width="0.5dp" android:layout_height="match_parent" android:layout_marginTop="5dp" android:layout_marginBottom="5dp"/>
            <com.baidu.navisdk.routetab.view.item.multi.MotorMultiRouteTabItem android:id="@+id/multi_tabs_three" android:visibility="gone" android:layout_marginLeft="2dp" android:layout_marginRight="2dp" style="@style/MultiRouteTabItem"/>
        </LinearLayout>
        <FrameLayout android:id="@+id/single_tab_container" android:layout_width="match_parent" android:layout_height="71dp" android:layout_marginTop="5dp">
            <com.baidu.navisdk.routetab.view.item.single.MotorSingleRouteTabItem android:id="@+id/single_tab" style="@style/SingleRouteTabItem"/>
        </FrameLayout>
    </FrameLayout>
    <View android:id="@+id/divider" android:background="#eaeaea" android:layout_width="match_parent" android:layout_height="1px" android:layout_below="@+id/route_tab_view"/>
    <com.baidu.navisdk.routetab.view.bar.MotorRRBottomBar android:id="@+id/bottom_bar" android:layout_width="match_parent" android:layout_height="58dp" android:layout_below="@+id/divider"/>
</merge>

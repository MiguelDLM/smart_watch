<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:mcpapp="http://schemas.android.com/apk/com.l4digital.fastscroll" xmlns:aapt="http://schemas.android.com/aapt" android:layout_width="match_parent" android:layout_height="@dimen/nsdk_future_trip_main_panel_mid_zone_height" android:layout_marginTop="@dimen/navi_dimens_1dp">
    <View android:id="@+id/top_line" android:visibility="visible" android:layout_marginLeft="@dimen/navi_dimens_12dp" android:layout_marginRight="@dimen/navi_dimens_12dp" android:layout_marginBottom="@dimen/navi_dimens_105dp" android:layout_alignParentBottom="true" style="@style/FutureTripMainPanelLineStyle"/>
    <View android:id="@+id/mid_line" android:visibility="visible" android:layout_marginLeft="@dimen/navi_dimens_12dp" android:layout_marginRight="@dimen/navi_dimens_12dp" android:layout_marginBottom="@dimen/navi_dimens_69dp" android:layout_alignParentBottom="true" style="@style/FutureTripMainPanelLineStyle"/>
    <View android:id="@+id/vertical_mid_line" android:background="#000000" android:visibility="visible" android:layout_width="1.5dp" android:layout_height="match_parent" android:layout_centerHorizontal="true"/>
    <androidx.recyclerview.widget.RecyclerView android:id="@+id/histogram" android:layout_width="match_parent" android:layout_height="match_parent"/>
    <androidx.recyclerview.widget.RecyclerView android:id="@+id/histogram_recall_fail" android:visibility="gone" android:layout_width="match_parent" android:layout_height="match_parent"/>
    <ImageView android:id="@+id/item_guide" android:background="@drawable/nsdk_drawable_future_trip_histogram_item_guide" android:visibility="gone" android:layout_width="@dimen/navi_dimens_107dp" android:layout_height="@dimen/navi_dimens_17dp" android:layout_marginLeft="@dimen/navi_dimens_74dp" android:layout_marginBottom="@dimen/navi_dimens_33dp" android:layout_alignParentBottom="true"/>
    <View android:id="@+id/bottom_divider" android:background="#dadada" android:layout_width="match_parent" android:layout_height="1px" android:layout_marginLeft="@dimen/navi_dimens_10dp" android:layout_marginRight="@dimen/navi_dimens_10dp" android:layout_marginBottom="@dimen/navi_dimens_33dp" android:layout_alignParentBottom="true"/>
    <ImageView android:id="@+id/select_item_time_iv" android:background="@drawable/nsdk_drawable_future_trip_item_select_time_bg" android:visibility="visible" android:layout_width="@dimen/nsdk_future_trip_select_time_width" android:layout_height="@dimen/nsdk_future_trip_select_time_height" android:layout_marginBottom="2dp" android:layout_alignParentBottom="true" android:layout_centerHorizontal="true"/>

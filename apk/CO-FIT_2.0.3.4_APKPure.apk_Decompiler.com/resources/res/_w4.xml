<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:mcpapp="http://schemas.android.com/apk/com.l4digital.fastscroll" xmlns:aapt="http://schemas.android.com/aapt" android:orientation="vertical" android:id="@+id/parking_item" android:layout_width="match_parent" android:layout_height="wrap_content">
    <LinearLayout android:orientation="horizontal" android:id="@+id/parking_content" android:layout_width="match_parent" android:layout_height="@dimen/navi_dimens_92dp">
        <LinearLayout android:layout_gravity="center_vertical" android:orientation="vertical" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_marginRight="@dimen/navi_dimens_5dp" android:layout_weight="1">
            <LinearLayout android:id="@+id/parking_first_line_content" android:layout_width="match_parent" android:layout_height="@dimen/navi_dimens_25dp">
                <ImageView android:layout_gravity="center_vertical" android:id="@+id/parking_icon" android:layout_width="@dimen/navi_dimens_20dp" android:layout_height="@dimen/navi_dimens_20dp" android:src="@drawable/nsdk_drawable_arrive_dest_park_one_red" android:scaleType="centerInside"/>
                <TextView android:textSize="@dimen/navi_dimens_16dp" android:textColor="@color/nsdk_cl_text_a" android:ellipsize="end" android:layout_gravity="center_vertical" android:id="@+id/parking_name" android:layout_width="match_parent" android:layout_height="wrap_content" android:singleLine="true"/>
            </LinearLayout>
            <LinearLayout android:orientation="horizontal" android:id="@+id/parking_second_line_content" android:layout_width="match_parent" android:layout_height="@dimen/navi_dimens_22dp">
                <TextView android:textSize="@dimen/navi_dimens_13dp" android:textColor="@color/nsdk_cl_text_i" android:layout_gravity="center_vertical" android:id="@+id/parking_number" android:layout_width="wrap_content" android:layout_height="wrap_content"/>
                <View android:id="@+id/parking_split_one" android:background="@color/nsdk_cl_bg_g" android:layout_width="1px" android:layout_height="match_parent" android:layout_marginLeft="@dimen/navi_dimens_4dp" android:layout_marginTop="@dimen/navi_dimens_5dp" android:layout_marginRight="@dimen/navi_dimens_4dp" android:layout_marginBottom="@dimen/navi_dimens_5dp"/>
                <TextView android:textSize="@dimen/navi_dimens_13dp" android:textColor="@color/nsdk_cl_text_i" android:layout_gravity="center_vertical" android:id="@+id/parking_distance" android:layout_width="wrap_content" android:layout_height="wrap_content"/>
                <View android:id="@+id/parking_split_two" android:background="@color/nsdk_cl_bg_g" android:layout_width="1px" android:layout_height="match_parent" android:layout_marginLeft="@dimen/navi_dimens_4dp" android:layout_marginTop="@dimen/navi_dimens_5dp" android:layout_marginRight="@dimen/navi_dimens_4dp" android:layout_marginBottom="@dimen/navi_dimens_5dp"/>
                <TextView android:textSize="@dimen/navi_dimens_13dp" android:textColor="@color/nsdk_cl_text_i" android:ellipsize="end" android:layout_gravity="center_vertical" android:id="@+id/parking_cost" android:layout_width="wrap_content" android:layout_height="wrap_content" android:singleLine="true"/>
            </LinearLayout>
            <LinearLayout android:orientation="horizontal" android:id="@+id/parking_third_line_content" android:layout_width="match_parent" android:layout_height="@dimen/navi_dimens_25dp">
                <TextView android:textSize="@dimen/navi_dimens_12dp" android:textColor="@color/nsdk_color_rg_arrive_dest_park_desc" android:layout_gravity="center_vertical" android:id="@+id/parking_label_desc" android:background="@drawable/nsdk_drawable_rg_arrive_dest_park_desc" android:paddingLeft="@dimen/navi_dimens_3dp" android:paddingTop="@dimen/navi_dimens_1dp" android:paddingRight="@dimen/navi_dimens_3dp" android:paddingBottom="@dimen/navi_dimens_1dp" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginRight="@dimen/navi_dimens_6dp" android:text="车位充足"/>
                <TextView android:textSize="@dimen/navi_dimens_12dp" android:textColor="@color/nsdk_cl_text_i" android:layout_gravity="center_vertical" android:id="@+id/parking_label_type" android:background="@drawable/nsdk_drawable_rg_arrive_dest_park_normal" android:paddingLeft="@dimen/navi_dimens_3dp" android:paddingTop="@dimen/navi_dimens_1dp" android:paddingRight="@dimen/navi_dimens_3dp" android:paddingBottom="@dimen/navi_dimens_1dp" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginRight="@dimen/navi_dimens_6dp" android:text="地上"/>
                <TextView android:textSize="@dimen/navi_dimens_12dp" android:textColor="@color/nsdk_cl_text_i" android:layout_gravity="center_vertical" android:id="@+id/parking_label_reservable" android:background="@drawable/nsdk_drawable_rg_arrive_dest_park_normal" android:paddingLeft="@dimen/navi_dimens_3dp" android:paddingTop="@dimen/navi_dimens_1dp" android:paddingRight="@dimen/navi_dimens_3dp" android:paddingBottom="@dimen/navi_dimens_1dp" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="预约"/>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout android:layout_gravity="center_vertical" android:orientation="vertical" android:id="@+id/parking_goto" android:layout_width="@dimen/navi_dimens_35dp" android:layout_height="wrap_content">
            <ImageView android:layout_gravity="center_horizontal" android:layout_width="@dimen/navi_dimens_25dp" android:layout_height="@dimen/navi_dimens_25dp" android:src="@drawable/nsdk_drawable_rg_arrive_dest_park_goto" android:scaleType="fitXY"/>
            <TextView android:textSize="@dimen/navi_dimens_11dp" android:textColor="@color/nsdk_cl_text_i" android:layout_gravity="center_horizontal" android:id="@+id/parking_goto_tv" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="@dimen/navi_dimens_3dp" android:text="到这去"/>
        </LinearLayout>
    </LinearLayout>
    <View android:id="@+id/parking_split_line" android:background="@color/nsdk_cl_bg_b" android:visibility="visible" android:layout_width="match_parent" android:layout_height="1px"/>
</LinearLayout>

<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:mcpapp="http://schemas.android.com/apk/com.l4digital.fastscroll" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/nsdk_rg_operable_notification_low_priority_confirm_background_pressed"/>
            <corners android:radius="50dp"/>
        </shape>
    </item>
    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <solid android:color="@color/nsdk_rg_operable_notification_low_priority_confirm_background"/>
            <corners android:radius="50dp"/>
        </shape>
    </item>
</selector>

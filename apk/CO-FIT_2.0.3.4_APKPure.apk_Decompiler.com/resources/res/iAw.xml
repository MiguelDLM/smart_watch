<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:mcpapp="http://schemas.android.com/apk/com.l4digital.fastscroll" xmlns:aapt="http://schemas.android.com/aapt">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/transparent"/>
        </shape>
    </item>
    <item android:left="-2dp" android:top="-2dp" android:right="-2dp">
        <shape>
            <solid android:color="@color/transparent"/>
            <stroke android:width="1dp" android:color="@color/line_color" android:dashWidth="0dp" android:dashGap="0dp"/>
        </shape>
    </item>
</layer-list>

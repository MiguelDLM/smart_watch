<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:id="@+id/car_energy_layout" android:layout_width="match_parent" android:layout_height="wrap_content">
    <TextView android:textSize="@dimen/navi_dimens_18dp" android:textColor="#ff333333" android:id="@+id/car_energy_type" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="动力类型"/>
    <LinearLayout android:gravity="center_vertical" android:orientation="horizontal" android:background="@drawable/nsdk_drawable_plate_normal_bg" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="@dimen/navi_dimens_15dp" android:layout_below="@+id/car_energy_type">
        <TextView android:id="@+id/energy_type_oil" android:text="油车" style="@style/EnergyType"/>
        <View android:id="@+id/divider_1" android:background="#ffd8d8d8" android:layout_width="@dimen/navi_dimens_1dp" android:layout_height="@dimen/navi_dimens_13dp"/>
        <TextView android:id="@+id/energy_type_electric" android:text="纯电动" android:layout_marginStart="@dimen/navi_dimens_1dp" style="@style/EnergyType"/>
        <View android:id="@+id/divider_2" android:background="#ffd8d8d8" android:layout_width="@dimen/navi_dimens_1dp" android:layout_height="@dimen/navi_dimens_13dp"/>
        <TextView android:id="@+id/energy_type_hybrid" android:text="混合动力" android:layout_marginStart="@dimen/navi_dimens_1dp" style="@style/EnergyType"/>
    </LinearLayout>
</RelativeLayout>

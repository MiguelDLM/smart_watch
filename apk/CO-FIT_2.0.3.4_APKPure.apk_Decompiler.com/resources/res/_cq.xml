<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:mcpapp="http://schemas.android.com/apk/com.l4digital.fastscroll" xmlns:aapt="http://schemas.android.com/aapt" android:id="@+id/base_plate_input_container" android:background="@drawable/bnav_bg_attribution_panel" android:layout_width="match_parent" android:layout_height="wrap_content">
    <RelativeLayout android:id="@+id/plate_btn_close_layout" android:layout_width="@dimen/navi_dimens_40dp" android:layout_height="@dimen/navi_dimens_40dp" android:layout_marginTop="@dimen/navi_dimens_10dp" android:layout_alignParentEnd="true">
        <ImageView android:layout_width="@dimen/navi_dimens_11dp" android:layout_height="@dimen/navi_dimens_11dp" android:src="@drawable/bnav_plate_limit_setting_close" android:layout_centerInParent="true" android:layout_marginEnd="@dimen/navi_dimens_19dp"/>
    </RelativeLayout>
    <RelativeLayout android:orientation="vertical" android:id="@+id/plate_input_main_panel" android:layout_width="match_parent" android:layout_height="wrap_content">
        <TextView android:textSize="@dimen/navi_dimens_19dp" android:textColor="#333333" android:gravity="center" android:id="@+id/plate_input_main_panel_title" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="@dimen/navi_dimens_20dp" android:text="添加车牌" android:fontFamily="sans-serif-medium" android:layout_marginStart="@dimen/navi_dimens_19dp"/>
        <View android:id="@+id/divider" android:background="#eeeeee" android:layout_width="match_parent" android:layout_height="1px" android:layout_marginTop="@dimen/navi_dimens_20dp" android:layout_below="@+id/plate_input_main_panel_title"/>
        <ScrollView android:id="@+id/scroll_plate_view" android:layout_width="match_parent" android:layout_height="@dimen/navi_dimens_228dp" android:layout_below="@+id/divider">
            <LinearLayout android:orientation="vertical" android:id="@+id/plate_layout" android:paddingBottom="@dimen/navi_dimens_20dp" android:layout_width="match_parent" android:layout_height="wrap_content" android:paddingStart="@dimen/navi_dimens_19dp" android:paddingEnd="@dimen/navi_dimens_19dp">
                <com.baidu.navisdk.module.plate.layout.CarTypeLayout android:id="@+id/nsdk_car_type" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="@dimen/navi_dimens_19dp"/>
                <com.baidu.navisdk.module.plate.layout.CarPlateLayout android:id="@+id/nsdk_car_plate_layout" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="@dimen/navi_dimens_10dp"/>
                <CheckBox android:textSize="@dimen/navi_dimens_12dp" android:textColor="#222222" android:id="@+id/bn_area_plate_select_item" android:background="@null" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="@dimen/navi_dimens_10dp" android:button="@null" android:text="此车为区域指标车" android:drawablePadding="@dimen/navi_dimens_4dp"/>
                <com.baidu.navisdk.module.plate.layout.CarEtcLayout android:id="@+id/nsdk_car_etc_layout" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="@dimen/navi_dimens_10dp"/>
                <View android:id="@+id/plate_divider" android:background="#e3e3e3" android:layout_width="match_parent" android:layout_height="1px" android:layout_marginTop="@dimen/navi_dimens_15dp"/>
                <com.baidu.navisdk.module.plate.layout.CarEnergyLayout android:id="@+id/nsdk_car_energy" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="@dimen/navi_dimens_19dp"/>
                <com.baidu.navisdk.module.plate.layout.DisplacementLayout android:id="@+id/nsdk_car_displacement" android:visibility="gone" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="@dimen/navi_dimens_10dp"/>
                <com.baidu.navisdk.module.plate.layout.TruckTypeLayout android:id="@+id/nsdk_truck_type" android:visibility="gone" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="@dimen/navi_dimens_19dp"/>
                <com.baidu.navisdk.module.plate.layout.TruckInfoLayout android:id="@+id/nsdk_truck_info" android:visibility="gone" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="@dimen/navi_dimens_19dp"/>
            </LinearLayout>
        </ScrollView>
        <View android:background="@drawable/bnav_plate_complete_btn_bg" android:layout_width="match_parent" android:layout_height="@dimen/navi_dimens_7dp" android:layout_above="@+id/complete_btn_layout"/>
        <RelativeLayout android:gravity="center_vertical" android:id="@+id/complete_btn_layout" android:layout_width="match_parent" android:layout_height="@dimen/navi_dimens_69dp" android:layout_below="@+id/scroll_plate_view" android:paddingStart="@dimen/navi_dimens_19dp" android:paddingEnd="@dimen/navi_dimens_19dp">
            <include layout="@layout/bnav_plate_input_panel_complete_btn_layout"/>
        </RelativeLayout>
    </RelativeLayout>
    <com.baidu.navisdk.module.plate.view.PlateAttributionView android:id="@+id/attribution_layout" android:visibility="gone" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_below="@+id/plate_input_main_panel"/>
</RelativeLayout>

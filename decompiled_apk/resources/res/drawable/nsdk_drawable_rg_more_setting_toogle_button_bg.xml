<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="false">
        <shape>
            <corners
                android:topLeftRadius="@dimen/navi_dimens_2dp"
                android:topRightRadius="@dimen/navi_dimens_2dp"
                android:bottomLeftRadius="@dimen/navi_dimens_2dp"
                android:bottomRightRadius="@dimen/navi_dimens_2dp"/>
            <solid android:color="@color/nsdk_cl_text_e"/>
            <stroke
                android:width="2px"
                android:color="#3385ff"/>
        </shape>
    </item>
    <item android:state_checked="true">
        <shape>
            <corners
                android:topLeftRadius="@dimen/navi_dimens_2dp"
                android:topRightRadius="@dimen/navi_dimens_2dp"
                android:bottomLeftRadius="@dimen/navi_dimens_2dp"
                android:bottomRightRadius="@dimen/navi_dimens_2dp"/>
            <solid android:color="#3385ff"/>
            <stroke
                android:width="2px"
                android:color="#3385ff"/>
        </shape>
    </item>
</selector>

<?xml version="1.0" encoding="utf-8"?>
<com.baidu.navisdk.module.carlogo.views.BN3DCarLogoPageItem xmlns:android="http://schemas.android.com/apk/res/android" xmlns:apps="http://schemas.android.com/apk/res-auto"
    android:layout_width="wrap_content"
    android:layout_height="match_parent">
    <ImageView
        android:layout_gravity="center"
        android:id="@+id/three_d_car_logo_item_img"
        android:layout_width="@dimen/navi_dimens_159dp"
        android:layout_height="@dimen/navi_dimens_110dp"
        android:scaleType="fitCenter"/>
    <com.baidu.navisdk.ui.widget.BNRoundProgress
        android:layout_gravity="center"
        android:id="@+id/three_d_car_logo_item_progress"
        android:background="@android:color/transparent"
        android:visibility="gone"
        android:layout_width="@dimen/navi_dimens_38dp"
        android:layout_height="@dimen/navi_dimens_38dp"
        apps:nsdk_bn_progress_color="#ffffff"
        apps:nsdk_bn_round_color="#33000000"
        apps:nsdk_bn_round_style="STROKE"
        apps:nsdk_bn_round_width="4dp"
        apps:nsdk_round_max_value="100"
        apps:nsdk_round_start_angle="0"
        apps:nsdk_round_text_color="#ffffff"
        apps:nsdk_round_text_size="@dimen/navi_dimens_13dp"/>
</com.baidu.navisdk.module.carlogo.views.BN3DCarLogoPageItem>

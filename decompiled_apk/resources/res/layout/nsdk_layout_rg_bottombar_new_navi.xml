<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/bn_rg_bottom_bar_eta_view"
    android:layout_width="match_parent"
    android:layout_height="@dimen/nsdk_rg_bottom_bar_new_height"
    app:layout_constraintBottom_toBottomOf="parent">
    <FrameLayout
        android:id="@+id/bn_rg_bottombar_new_road_ly"
        android:layout_width="match_parent"
        android:layout_height="@dimen/navi_dimens_46dp"
        app:layout_constraintTop_toTopOf="parent"/>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/bn_rg_bottombar_new_ly"
        android:layout_width="match_parent"
        android:layout_height="@dimen/nsdk_rg_bottom_bar_new_eta_height"
        app:layout_constraintBottom_toBottomOf="parent">
        <include
            android:id="@+id/bn_rg_bottombar_new_left_ly"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/navi_dimens_48dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            layout="@layout/nsdk_layout_rg_bottombar_new_left_btn"/>
        <include
            android:id="@+id/bn_rg_bottombar_new_right_ly"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/navi_dimens_48dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            layout="@layout/nsdk_layout_rg_bottombar_new_right_btn"/>
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/bnav_rg_new_eta_cpntainer"
            android:layout_width="0dp"
            android:layout_height="@dimen/navi_dimens_48dp"
            android:layout_marginStart="@dimen/navi_dimens_10dp"
            android:layout_marginEnd="@dimen/navi_dimens_10dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/bn_rg_bottombar_new_right_ly"
            app:layout_constraintStart_toEndOf="@+id/bn_rg_bottombar_new_left_ly"
            app:layout_constraintTop_toTopOf="parent">
            <include
                android:id="@+id/bn_rg_bottombar_new_eta_ly"
                android:visibility="visible"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                layout="@layout/nsdk_layout_rg_bottombar_new_eta_view"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
        <include
            android:id="@+id/bn_rg_bottombar_new_continue_ly"
            android:visibility="gone"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            app:layout_constraintEnd_toStartOf="@+id/bn_rg_bottombar_new_right_ly"
            app:layout_constraintStart_toEndOf="@+id/bn_rg_bottombar_new_left_ly"
            app:layout_constraintTop_toTopOf="parent"
            layout="@layout/nsdk_layout_rg_bottombar_new_continue_view"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>

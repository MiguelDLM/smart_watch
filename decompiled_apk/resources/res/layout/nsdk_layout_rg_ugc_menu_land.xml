<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:background="#00000000"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <RelativeLayout
        android:id="@+id/bnav_rg_part_trans_bground"
        android:background="#00000000"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@+id/bnav_rg_ugc_menu_items"/>
    <LinearLayout
        android:orientation="vertical"
        android:id="@+id/bnav_rg_ugc_menu_items"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true">
        <LinearLayout
            android:orientation="vertical"
            android:id="@+id/bnav_rg_menu_ugc_func_panel"
            android:background="@color/nsdk_cl_bg_d"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <View
                android:id="@+id/bnav_rg_menu_h_divider_0"
                android:background="@color/nsdk_cl_bg_b"
                android:layout_width="match_parent"
                android:layout_height="@dimen/navi_dimens_1dp"/>
            <RelativeLayout
                android:id="@+id/bnav_rg_menu_route_block_item"
                android:background="@drawable/nsdk_drawable_common_bg_pressed_mask_selector"
                android:layout_width="match_parent"
                android:layout_height="@dimen/nsdk_rg_ugc_menu_item_height"
                android:minHeight="@dimen/nsdk_rg_ugc_menu_item_height">
                <TextView
                    android:id="@+id/bnav_rg_menu_route_block_item_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/nsdk_rg_text_margin"
                    android:text="@string/nsdk_string_route_block"
                    android:layout_alignParentLeft="true"
                    android:layout_centerInParent="true"
                    style="@style/Text_NaviUGCSettingsText"/>
                <ImageView
                    android:gravity="center_vertical"
                    android:background="@android:color/transparent"
                    android:layout_width="@dimen/navi_dimens_24dp"
                    android:layout_height="@dimen/navi_dimens_24dp"
                    android:layout_marginRight="@dimen/navi_dimens_8dp"
                    android:src="@drawable/nsdk_drawable_common_ic_list_right_arrow"
                    android:scaleType="fitXY"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"/>
            </RelativeLayout>
            <View
                android:id="@+id/bnav_rg_menu_h_divider_1"
                android:background="@color/nsdk_cl_bg_b"
                android:layout_width="match_parent"
                android:layout_height="@dimen/navi_dimens_1dp"/>
            <RelativeLayout
                android:id="@+id/bnav_rg_menu_trafic_flag_error_item"
                android:background="@drawable/nsdk_drawable_common_bg_pressed_mask_selector"
                android:layout_width="match_parent"
                android:layout_height="@dimen/nsdk_rg_ugc_menu_item_height"
                android:minHeight="@dimen/nsdk_rg_ugc_menu_item_height">
                <TextView
                    android:id="@+id/bnav_rg_menu_trafic_flag_error_item_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/nsdk_rg_text_margin"
                    android:text="@string/nsdk_string_trafic_flag_error"
                    android:layout_alignParentLeft="true"
                    android:layout_centerInParent="true"
                    style="@style/Text_NaviUGCSettingsText"/>
                <ImageView
                    android:gravity="center_vertical"
                    android:background="@android:color/transparent"
                    android:layout_width="@dimen/navi_dimens_24dp"
                    android:layout_height="@dimen/navi_dimens_24dp"
                    android:layout_marginRight="@dimen/navi_dimens_8dp"
                    android:src="@drawable/nsdk_drawable_common_ic_list_right_arrow"
                    android:scaleType="fitXY"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"/>
            </RelativeLayout>
            <View
                android:id="@+id/bnav_rg_menu_h_divider_2"
                android:background="@color/nsdk_cl_bg_b"
                android:layout_width="match_parent"
                android:layout_height="@dimen/navi_dimens_1dp"/>
            <RelativeLayout
                android:id="@+id/bnav_rg_menu_route_bad_item"
                android:background="@drawable/nsdk_drawable_common_bg_pressed_mask_selector"
                android:layout_width="match_parent"
                android:layout_height="@dimen/nsdk_rg_ugc_menu_item_height"
                android:minHeight="@dimen/nsdk_rg_ugc_menu_item_height">
                <TextView
                    android:id="@+id/bnav_rg_menu_route_bad_item_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/nsdk_rg_text_margin"
                    android:text="@string/nsdk_string_route_bad"
                    android:layout_alignParentLeft="true"
                    android:layout_centerInParent="true"
                    style="@style/Text_NaviUGCSettingsText"/>
                <ImageView
                    android:gravity="center_vertical"
                    android:background="@android:color/transparent"
                    android:layout_width="@dimen/navi_dimens_24dp"
                    android:layout_height="@dimen/navi_dimens_24dp"
                    android:layout_marginRight="@dimen/navi_dimens_8dp"
                    android:src="@drawable/nsdk_drawable_common_ic_list_right_arrow"
                    android:scaleType="fitXY"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"/>
            </RelativeLayout>
            <View
                android:id="@+id/bnav_rg_menu_h_divider_3"
                android:background="@color/nsdk_cl_bg_b"
                android:layout_width="match_parent"
                android:layout_height="@dimen/navi_dimens_1dp"/>
            <RelativeLayout
                android:id="@+id/bnav_rg_menu_route_added_item"
                android:background="@drawable/nsdk_drawable_common_bg_pressed_mask_selector"
                android:layout_width="match_parent"
                android:layout_height="@dimen/nsdk_rg_ugc_menu_item_height"
                android:minHeight="@dimen/nsdk_rg_ugc_menu_item_height">
                <TextView
                    android:id="@+id/bnav_rg_menu_route_added_item_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/nsdk_rg_text_margin"
                    android:text="@string/nsdk_string_route_added"
                    android:layout_alignParentLeft="true"
                    android:layout_centerInParent="true"
                    style="@style/Text_NaviUGCSettingsText"/>
                <ImageView
                    android:gravity="center_vertical"
                    android:background="@android:color/transparent"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="@dimen/navi_dimens_8dp"
                    android:src="@drawable/nsdk_drawable_common_ic_list_right_arrow"
                    android:scaleType="fitXY"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"/>
            </RelativeLayout>
            <View
                android:id="@+id/bnav_rg_menu_h_divider_4"
                android:background="@color/nsdk_cl_bg_b"
                android:layout_width="match_parent"
                android:layout_height="@dimen/navi_dimens_1dp"/>
        </LinearLayout>
        <RelativeLayout
            android:id="@+id/bnav_rg_close_content_panel"
            android:background="@color/nsdk_cl_bg_d"
            android:layout_width="match_parent"
            android:layout_height="@dimen/navi_dimens_80dp">
            <Button
                android:id="@+id/bnav_rg_menu_ugc_close"
                android:background="@drawable/bnav_lineframe_button_selector"
                android:layout_width="match_parent"
                android:layout_height="@dimen/navi_dimens_48dp"
                android:layout_marginLeft="@dimen/navi_dimens_16dp"
                android:layout_marginTop="@dimen/navi_dimens_16dp"
                android:layout_marginRight="@dimen/navi_dimens_16dp"
                android:layout_marginBottom="@dimen/navi_dimens_16dp"
                android:layout_alignParentBottom="true"/>
            <TextView
                android:textSize="16sp"
                android:textColor="@color/nsdk_cl_text_a"
                android:gravity="center"
                android:id="@+id/bnav_rg_menu_close_tv"
                android:layout_width="match_parent"
                android:layout_height="@dimen/navi_dimens_48dp"
                android:layout_marginLeft="@dimen/navi_dimens_16dp"
                android:layout_marginTop="@dimen/navi_dimens_16dp"
                android:layout_marginRight="@dimen/navi_dimens_16dp"
                android:layout_marginBottom="@dimen/navi_dimens_16dp"
                android:text="@string/nsdk_string_close"
                android:layout_alignParentBottom="true"/>
        </RelativeLayout>
    </LinearLayout>
</RelativeLayout>

<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingStart="@dimen/navi_dimens_16dp">
    <TextView
        android:textSize="@dimen/navi_dimens_14dp"
        android:textColor="@color/nsdk_cl_text_b_mm_title"
        android:id="@+id/bnav_rg_bottombar_new_left_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="更多"
        android:drawableTop="@drawable/bnav_bottom_new_left_more"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>
    <View
        android:id="@+id/bnav_rg_bottombar_new_left_divider"
        android:background="@color/nsdk_cl_text_bg_divider"
        android:layout_width="@dimen/navi_dimens_1dp"
        android:layout_height="@dimen/navi_dimens_38dp"
        android:layout_marginStart="@dimen/navi_dimens_16dp"
        app:layout_constraintBottom_toBottomOf="@+id/bnav_rg_bottombar_new_left_btn"
        app:layout_constraintStart_toEndOf="@+id/bnav_rg_bottombar_new_left_btn"
        app:layout_constraintTop_toTopOf="@+id/bnav_rg_bottombar_new_left_btn"/>
</androidx.constraintlayout.widget.ConstraintLayout>

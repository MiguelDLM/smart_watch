<?xml version="1.0" encoding="utf-8"?>
<com.baidu.navisdk.module.newguide.widgets.ServiceAreaBaseRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/bnavi_hw_service_area_content_layout"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">
    <LinearLayout
        android:orientation="vertical"
        android:id="@+id/bnavi_hw_service_area_top_layout"
        android:background="@drawable/nsdk_drawable_rg_hw_bg_green_top"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    <LinearLayout
        android:orientation="vertical"
        android:id="@+id/bnavi_hw_service_area_bottom_layout"
        android:background="@drawable/nsdk_drawable_rg_hw_bg_blue_top"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="-5dp"
        android:layout_below="@+id/bnavi_hw_service_area_top_layout"/>
</com.baidu.navisdk.module.newguide.widgets.ServiceAreaBaseRelativeLayout>

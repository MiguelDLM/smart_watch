<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/bnav_rg_toolbox_via_time_and_dist_ly"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:gravity="center"
        android:id="@+id/bnav_rg_toolbox_via_time_and_dist_ll"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toTopOf="@+id/bnav_rg_toolbox_via_remain_time_and_dist_ll"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">
        <include
            android:id="@+id/bnav_rg_toolbox_via_rookie_mode_tag"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/navi_dimens_17dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_chainStyle="packed"
            layout="@layout/nsdk_layout_rg_new_rookie_mode_tag"/>
        <TextView
            android:textSize="@dimen/navi_dimens_20dp"
            android:textStyle="bold"
            android:textColor="@color/nsdk_cl_text_b_mm_title"
            android:gravity="center"
            android:id="@+id/bnav_rg_toolbox_via_arrive_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="最近途经点"
            android:layout_marginStart="@dimen/navi_dimens_5dp"
            app:layout_constraintStart_toEndOf="@+id/bnav_rg_toolbox_via_rookie_mode_tag"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_chainStyle="packed"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/bnav_rg_toolbox_via_remain_time_and_dist_ll"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bnav_rg_toolbox_via_time_and_dist_ll">
        <TextView
            android:textSize="@dimen/navi_dimens_19dp"
            android:textStyle="bold"
            android:textColor="@color/nsdk_cl_text_b_mm_title"
            android:id="@+id/bnav_rg_toolbox_via_remain_time_and_dist"
            android:visibility="visible"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:singleLine="true"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"/>
        <TextView
            android:textSize="@dimen/navi_dimens_18dp"
            android:textStyle="bold"
            android:textColor="@color/nsdk_cl_text_b_mm_title"
            android:gravity="center"
            android:id="@+id/bnav_rg_toolbox_via_traffic_light_count"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/nsdk_rg_toolbox_traffic_light_margin_left"
            android:singleLine="true"
            android:drawableLeft="@drawable/nsdk_drawable_toolbox_traffic_light_portrait"
            android:drawablePadding="@dimen/navi_dimens_2dp"
            app:layout_constraintStart_toEndOf="@+id/bnav_rg_toolbox_via_remain_time_and_dist"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>

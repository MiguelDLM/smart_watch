<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:id="@+id/root"
    android:background="@color/view_page_main_bg"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <include layout="@layout/layout_title_iv_tv_iv"/>
    <com.sma.smartv3.view.AbhSettingsHeader
        android:id="@+id/item_header"
        app:abh_description="@string/game_set_tip"
        app:abh_title="@string/game_control"
        style="@style/abh_settings_item"/>
    <com.sma.smartv3.view.AbhSettingsItem
        android:id="@+id/item_time_range1"
        android:onClick="onSetTimeRange1"
        app:abh_left_text="@string/time"
        style="@style/abh_settings_item"/>
    <com.sma.smartv3.view.AbhSettingsItem
        android:id="@+id/item_time_range2"
        android:onClick="onSetTimeRange2"
        app:abh_left_text="@string/time"
        style="@style/abh_settings_item"/>
</LinearLayout>

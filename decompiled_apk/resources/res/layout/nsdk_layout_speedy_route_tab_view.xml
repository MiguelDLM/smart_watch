<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android">
    <FrameLayout
        android:id="@+id/route_tab_view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <LinearLayout
            android:orientation="horizontal"
            android:id="@+id/multi_tabs_container"
            android:layout_width="match_parent"
            android:layout_height="86dp">
            <com.baidu.navisdk.routetab.view.item.multi.SpeedyMultiRouteTabItem
                android:gravity="center"
                android:orientation="vertical"
                android:id="@+id/multi_tabs_one"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"/>
            <com.baidu.navisdk.routetab.view.item.multi.SpeedyMultiRouteTabItem
                android:gravity="center"
                android:orientation="vertical"
                android:id="@+id/multi_tabs_two"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"/>
            <com.baidu.navisdk.routetab.view.item.multi.SpeedyMultiRouteTabItem
                android:gravity="center"
                android:orientation="vertical"
                android:id="@+id/multi_tabs_three"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"/>
        </LinearLayout>
        <FrameLayout
            android:id="@+id/single_tab_container"
            android:layout_width="match_parent"
            android:layout_height="63dp">
            <com.baidu.navisdk.routetab.view.item.single.SpeedySingleRouteTabItem
                android:id="@+id/single_tab"
                style="@style/SingleRouteTabItem"/>
        </FrameLayout>
    </FrameLayout>
    <View
        android:id="@+id/divider"
        android:background="#eaeaea"
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:layout_below="@+id/route_tab_view"/>
    <com.baidu.navisdk.routetab.view.bar.MotorRRBottomBar
        android:id="@+id/bottom_bar"
        android:layout_width="match_parent"
        android:layout_height="58dp"
        android:layout_below="@+id/divider"/>
</merge>

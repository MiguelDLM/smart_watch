<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto">
    <FrameLayout
        android:id="@+id/tabs_container"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <LinearLayout
            android:orientation="horizontal"
            android:id="@+id/multi_tabs_container"
            android:layout_width="match_parent"
            android:layout_height="76dp"
            android:layout_marginLeft="5dp"
            android:layout_marginTop="7dp"
            android:layout_marginRight="5dp"
            android:layout_marginBottom="7dp">
            <com.baidu.navisdk.routetab.view.item.multi.NewEngMultiRouteTabItem
                android:id="@+id/multi_tabs_one"
                android:layout_marginLeft="2dp"
                android:layout_marginRight="2dp"
                app:bn_routetab_describe_color="@color/nsdk_ne_route_result_tab_traffic_info_selector"
                app:bn_routetab_distance_color="@color/nsdk_ne_route_result_tab_traffic_info_selector"
                app:bn_routetab_time_color="@color/nsdk_ne_route_result_car_tab_time_selector"
                app:bn_routetab_tolls_ic="@drawable/nsdk_drawable_ne_route_result_road_toll_selector"
                app:bn_routetab_traffic_ic="@drawable/nsdk_drawable_ne_route_result_traffic_light_selector"
                style="@style/MultiRouteTabItem"/>
            <View
                android:id="@+id/multi_tabs_line1"
                android:background="#e9eaf0"
                android:layout_width="0.5dp"
                android:layout_height="match_parent"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="5dp"/>
            <com.baidu.navisdk.routetab.view.item.multi.NewEngMultiRouteTabItem
                android:id="@+id/multi_tabs_two"
                android:layout_marginLeft="2dp"
                android:layout_marginRight="2dp"
                app:bn_routetab_describe_color="@color/nsdk_ne_route_result_tab_traffic_info_selector"
                app:bn_routetab_distance_color="@color/nsdk_ne_route_result_tab_traffic_info_selector"
                app:bn_routetab_time_color="@color/nsdk_ne_route_result_car_tab_time_selector"
                app:bn_routetab_tolls_ic="@drawable/nsdk_drawable_ne_route_result_road_toll_selector"
                app:bn_routetab_traffic_ic="@drawable/nsdk_drawable_ne_route_result_traffic_light_selector"
                style="@style/MultiRouteTabItem"/>
            <View
                android:id="@+id/multi_tabs_line2"
                android:background="#e9eaf0"
                android:layout_width="0.5dp"
                android:layout_height="match_parent"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="5dp"/>
            <com.baidu.navisdk.routetab.view.item.multi.NewEngMultiRouteTabItem
                android:id="@+id/multi_tabs_three"
                android:layout_marginLeft="2dp"
                android:layout_marginRight="2dp"
                app:bn_routetab_describe_color="@color/nsdk_ne_route_result_tab_traffic_info_selector"
                app:bn_routetab_distance_color="@color/nsdk_ne_route_result_tab_traffic_info_selector"
                app:bn_routetab_time_color="@color/nsdk_ne_route_result_car_tab_time_selector"
                app:bn_routetab_tolls_ic="@drawable/nsdk_drawable_ne_route_result_road_toll_selector"
                app:bn_routetab_traffic_ic="@drawable/nsdk_drawable_ne_route_result_traffic_light_selector"
                style="@style/MultiRouteTabItem"/>
        </LinearLayout>
        <FrameLayout
            android:id="@+id/single_tab_container"
            android:layout_width="match_parent"
            android:layout_height="66dp"
            android:layout_marginLeft="5dp"
            android:layout_marginTop="7dp"
            android:layout_marginRight="5dp"
            android:layout_marginBottom="7dp">
            <com.baidu.navisdk.routetab.view.item.single.NewEngSingleRouteTabItem
                android:id="@+id/single_tab"
                android:background="@drawable/nsdk_drawable_tab_bg_neweng"
                android:layout_marginLeft="2dp"
                android:layout_marginRight="2dp"
                app:bn_routetab_describe_color="@color/nsdk_ne_route_result_tab_traffic_info_selector"
                app:bn_routetab_distance_color="@color/nsdk_ne_route_result_tab_traffic_info_selector"
                app:bn_routetab_time_color="@color/nsdk_ne_route_result_car_tab_time_selector"
                app:bn_routetab_tolls_ic="@drawable/nsdk_drawable_ne_route_result_road_toll_selector"
                app:bn_routetab_traffic_ic="@drawable/nsdk_drawable_ne_route_result_traffic_light_selector"
                style="@style/SingleRouteTabItem"/>
        </FrameLayout>
    </FrameLayout>
</merge>

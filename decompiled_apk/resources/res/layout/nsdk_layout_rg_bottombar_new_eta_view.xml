<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_constraintBottom_toBottomOf="parent">
    <include
        android:id="@+id/bnav_rg_new_rec_parking_ll"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        layout="@layout/nsdk_layout_rg_bottombar_new_rec_parking"/>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/bnav_rg_new_eta_time_ll"
        android:visibility="visible"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">
        <include
            android:id="@+id/bnav_rg_toolbox_time_and_dist_ly"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            layout="@layout/bnav_layout_rg_bottombar_destination_eta"/>
        <ViewStub
            android:id="@+id/bnav_rg_toolbox_via_time_dist_ly_stub"
            android:layout="@layout/bnav_layout_rg_bottombar_via_eta"
            android:inflatedId="@+id/bnav_rg_toolbox_via_time_and_dist_ly"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:layout_constraintBottom_toBottomOf="@+id/bnav_rg_toolbox_time_and_dist_ly"
            app:layout_constraintEnd_toEndOf="@+id/bnav_rg_toolbox_time_and_dist_ly"
            app:layout_constraintStart_toStartOf="@+id/bnav_rg_toolbox_time_and_dist_ly"
            app:layout_constraintTop_toTopOf="@+id/bnav_rg_toolbox_time_and_dist_ly"/>
        <ViewStub
            android:id="@+id/bnav_rg_bmbar_driving_info_ly_stub"
            android:visibility="gone"
            android:layout="@layout/bnav_layout_rg_bottombar_driving_distance_time"
            android:inflatedId="@+id/bnav_rg_bmbar_driving_info_ly"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:layout_constraintBottom_toBottomOf="@+id/bnav_rg_toolbox_time_and_dist_ly"
            app:layout_constraintEnd_toEndOf="@+id/bnav_rg_toolbox_time_and_dist_ly"
            app:layout_constraintStart_toStartOf="@+id/bnav_rg_toolbox_time_and_dist_ly"
            app:layout_constraintTop_toTopOf="@+id/bnav_rg_toolbox_time_and_dist_ly"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>

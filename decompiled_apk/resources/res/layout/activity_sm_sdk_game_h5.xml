<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <WebView
        android:id="@+id/wb_common"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
    <TextView
        android:textSize="14sp"
        android:textColor="@color/white"
        android:layout_gravity="center_vertical"
        android:id="@+id/tv_data"
        android:background="#66000000"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="16dp"/>
    <FrameLayout
        android:id="@+id/fl_loading"
        android:background="@drawable/sm_loading_bg"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <ImageView
            android:layout_gravity="center_horizontal"
            android:id="@+id/iv_logo"
            android:layout_width="@dimen/sm_sdk_px_179"
            android:layout_height="@dimen/sm_sdk_px_120"
            android:layout_marginTop="@dimen/sm_sdk_px_50"
            android:src="@drawable/sm_logo"/>
        <FrameLayout
            android:layout_gravity="bottom|center_horizontal"
            android:layout_width="match_parent"
            android:layout_height="@dimen/sm_sdk_px_20"
            android:layout_marginBottom="@dimen/sm_sdk_px_32">
            <ImageView
                android:layout_gravity="center_horizontal"
                android:id="@+id/vv_loading_one"
                android:background="@drawable/sm_load_bg"
                android:layout_width="match_parent"
                android:layout_height="@dimen/sm_sdk_px_20"
                android:layout_marginLeft="@dimen/sm_sdk_px_85"
                android:layout_marginRight="@dimen/sm_sdk_px_85"/>
            <ImageView
                android:layout_gravity="center_vertical"
                android:id="@+id/vv_loading_two"
                android:background="@drawable/sm_load_progress"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/sm_sdk_px_13"
                android:layout_marginLeft="@dimen/sm_sdk_px_87"/>
            <TextView
                android:textSize="@dimen/sm_sdk_px_14"
                android:textColor="@color/white"
                android:gravity="center"
                android:layout_gravity="center_horizontal"
                android:id="@+id/tv_progress"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/sm_sdk_px_20"
                android:text="0%"/>
        </FrameLayout>
        <ImageView
            android:layout_gravity="bottom"
            android:id="@+id/vv_loading_hulu"
            android:background="@drawable/animation_26_0001"
            android:layout_width="@dimen/sm_sdk_px_90"
            android:layout_height="@dimen/sm_sdk_px_40"
            android:layout_marginLeft="@dimen/sm_sdk_px_23"
            android:layout_marginBottom="@dimen/sm_sdk_px_50"/>
    </FrameLayout>
    <FrameLayout
        android:id="@+id/fl_loading_portrait"
        android:background="@drawable/sm_loading_bg"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <ImageView
            android:layout_gravity="center_horizontal"
            android:id="@+id/iv_logo_portrait"
            android:layout_width="@dimen/sm_sdk_px_179"
            android:layout_height="@dimen/sm_sdk_px_120"
            android:layout_marginTop="@dimen/sm_sdk_px_112"
            android:src="@drawable/sm_logo"/>
        <FrameLayout
            android:layout_gravity="bottom|center_horizontal"
            android:layout_width="match_parent"
            android:layout_height="@dimen/sm_sdk_px_20"
            android:layout_marginBottom="@dimen/sm_sdk_px_76">
            <ImageView
                android:id="@+id/vv_loading_one_portrait"
                android:background="@drawable/sm_load_bg"
                android:layout_width="match_parent"
                android:layout_height="@dimen/sm_sdk_px_20"
                android:layout_marginLeft="@dimen/sm_sdk_px_25"
                android:layout_marginRight="@dimen/sm_sdk_px_25"/>
            <ImageView
                android:layout_gravity="center_vertical"
                android:id="@+id/vv_loading_two_portrait"
                android:background="@drawable/sm_load_progress"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/sm_sdk_px_13"
                android:layout_marginLeft="@dimen/sm_sdk_px_27"/>
            <TextView
                android:textSize="@dimen/sm_sdk_px_14"
                android:textColor="@color/white"
                android:gravity="center"
                android:layout_gravity="center_horizontal"
                android:id="@+id/tv_progress_portrait"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/sm_sdk_px_20"
                android:text="0%"/>
        </FrameLayout>
        <ImageView
            android:layout_gravity="bottom"
            android:id="@+id/vv_loading_hulu_portrait"
            android:background="@drawable/animation_26_0001"
            android:layout_width="@dimen/sm_sdk_px_90"
            android:layout_height="@dimen/sm_sdk_px_40"
            android:layout_marginLeft="-30dp"
            android:layout_marginBottom="@dimen/sm_sdk_px_98"/>
    </FrameLayout>
    <ImageView
        android:id="@+id/iv_cancel"
        android:visibility="gone"
        android:layout_width="@dimen/sm_sdk_px_40"
        android:layout_height="@dimen/sm_sdk_px_40"
        android:layout_marginLeft="@dimen/sm_sdk_px_21"
        android:layout_marginTop="@dimen/sm_sdk_px_21"
        android:src="@drawable/sm_cancel_btn"/>
</FrameLayout>
